    <origin rpy="0 0 0" xyz="0 0 0.058"/>
    <parent link="right_outer_bar"/>
    <child link="right_finger_mount"/>
    <axis xyz="1 0 0"/>
    <limit effort="0" lower="-0.7854" upper="0.155" velocity="2.0"/>
